{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:prod": {
      "dependsOn": ["^build:prod"],
      "outputs": ["./vite-app/dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
